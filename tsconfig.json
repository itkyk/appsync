{
  "compilerOptions": {
    "lib": ["ES2022"],
    "target": "ESNext",
    "module": "commonjs",
     "rootDir": "./src",
     "sourceMap": false,
     "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
